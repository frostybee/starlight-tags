{
  "name": "starlight-tags",
  "version": "0.0.1",
  "license": "MIT",
  "description": "A plugin to add tags to your Starlight docs.",
  "author": "frostybee",
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./schema": {
      "types": "./src/schemas/frontmatter.d.ts",
      "import": "./src/schemas/frontmatter.js"
    },
    "./components": {
      "types": "./src/components/index.d.ts",
      "import": "./src/components/index.js"
    },
    "./styles": "./src/styles/tags.css",
    "./pages/tags-index.astro": "./src/pages/tags-index.astro",
    "./pages/tag-page.astro": "./src/pages/tag-page.astro"
  },
  "files": [
    "*.js",
    "*.d.ts",
    "src/"
  ],
  "keywords": [
    "astro",
    "starlight",
    "astro-integration",
    "documentation",
    "tags",
    "plugin"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "lint": "eslint .",
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@astrojs/starlight": "^0.35.3",
    "astro": "^5.13.7",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^18.0.0",    
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@astrojs/starlight": ">=0.35",
    "astro": ">=5.13.0"
    
  },
  "engines": {
    "node": "^18.17.1 || ^20.3.0 || >=21.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
    "homepage": "https://github.com/frostybee/starlight-tags",
  "repository": {
    "type": "git",
    "url": "https://github.com/frostybee/starlight-tags.git",
    "directory": "packages/starlight-tags"
  },
  "bugs": "https://github.com/frostybee/starlight-tags/issues"
}
